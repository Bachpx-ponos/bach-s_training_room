{
  "manifest_version": 3,
  "name": "BachsCrawlHistoryExtension",
  "description": "Lay history tren binace",
  "version": "1.0",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/lib/lodash.js",
        "scripts/lib/jquery.js",
        "scripts/settings.js",
        "scripts/content.js"
      ],
      "css": [
        "styles/lib/bootstrap.css",
        "styles/content.css"
      ],
      "matches": [
        "https://www.binance.com/vi/futures/*"
      ]
    }
  ]
}
